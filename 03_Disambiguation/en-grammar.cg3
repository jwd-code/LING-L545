DELIMITERS = "<.>" ;

LIST VERB = VERB ;
LIST DET =  DET;
LIST NOUN = NOUN ;
LIST ADJ = adj ;
LIST PRON = PRON ; 
LIST PROPN = PROPN ;
LIST CCONJ = CCONJ ;
LIST PUNCT = PUNCT ;
LIST ADV = ADV ;



SELECT VERB IF (1 VERB) ;
SELECT VERB IF (-1 VERB) ; 
SELECT VERB IF (-1 PRON) ;
SELECT DET IF (-1 VERB) (1 NOUN) ;
SELECT DET IF (-1 VERB) (1 PROPN) ;
SELECT CCONJ IF (-1 PUNCT) (1 ADV) ;
